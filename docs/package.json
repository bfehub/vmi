    "@bfehub/vuepress-theme-vmi": "workspace:*",
    "@vuepress/client": "2.0.0-beta.48",
    "@vuepress/plugin-search": "2.0.0-beta.48",
    "@vuepress/theme-default": "2.0.0-beta.48",
    "anywhere": "^1.5.0",
    "element-plus": "^2.2.2",
    "vue": "^3.2.33",
    "vuepress": "2.0.0-beta.48"
  }
