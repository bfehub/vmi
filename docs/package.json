{
  "name": "docs",
  "private": true,
  "scripts": {
    "docs:build": "vuepress build --clean-cache",
    "docs:clean": "rimraf .vuepress/.temp .vuepress/.cache .vuepress/dist",
    "docs:dev": "vuepress dev --clean-cache",
    "docs:serve": "anywhere -s -h localhost -d .vuepress/dist"
  },
  "dependencies": {
    "@bfehub/vuepress-components": "workspace:*",
    "@vuepress/client": "2.0.0-beta.45",
    "@vuepress/theme-default": "2.0.0-beta.45",
    "anywhere": "^1.5.0",
    "vue": "^3.2.33",
    "vuepress": "2.0.0-beta.45"
  }
}
